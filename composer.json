{
  "require": {
    "doctrine/orm": "*",
    "doctrine/annotations": "*",
    "knplabs/github-api": "*",
    "mpratt/embera": "*",
    "nyholm/psr7": "*",
    "symfony/cache": "*",
    "symfony/expression-language": "*",
    "symfony/form": "*",
    "symfony/http-client": "*",
    "symfony/http-foundation": "*",
    "symfony/mailer": "*",
    "symfony/translation": "*",
    "symfony/twig-bridge": "*",
    "twig/twig": "*"
  },
  "require-dev": {
    "symfony/messenger": "*"
  },
  "autoload": {
    "psr-4": {"": "entities"}
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    },
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
